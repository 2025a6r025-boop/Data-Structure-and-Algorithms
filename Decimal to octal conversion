// Decimal to octal conversion.
#include<stdio.h>
int main (){
    int num,oct=0,place=1;
    printf("Enter a number: ");
    scanf("%d",&num);
    int temp=num;
    while(num!=0){
    int rem=num%8;
    oct=oct+rem*place;
    num=num/8;
    place=place*10;
    }
    printf("Octal of %d is %d",num,oct);
    return 0;
}
